{
  "$schema":"http://json-schema.org/draft-07/schema#",
  "$id":"post_mask.schema.json",
  "title":"post_mask",
  "description":"Initiates an asynchronous job to apply xmp data to an image",
  "type":"object",
  "required":[
    "options"
  ],
  "properties":{
    "options":{
      "type":"object",
      "minProperties":1,
      "required":[
        "maskType",
        "version"
      ],
      "properties":{
        "maskType":{
          "type":"string",
          "description": "The type of mask to generate."
        },
        "version":{
          "type":"string",
          "description": "The version of Camera RAW to use while generating the mask."
        }
      },
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
